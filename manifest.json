{
    "name": "Gruntled",
    "version": "0.1",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "permissions": [
        "scripting",
        "activeTab",
        "tabs",
        "storage",
        "identity"
    ],
    "action": {
        "default_popup": "popup.html"
    }
}
